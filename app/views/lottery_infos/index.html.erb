<p id="notice"><%= notice %></p>

<h1>Lottery Infos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Store</th>
      <th>Lottery type</th>
      <th>Lottery date</th>
      <th>Purchase cost</th>
      <th>Lottery number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lottery_infos.each do |lottery_info| %>
      <tr>
        <td><%= lottery_info.user %></td>
        <td><%= lottery_info.store %></td>
        <td><%= lottery_info.lottery_type %></td>
        <td><%= lottery_info.lottery_date %></td>
        <td><%= lottery_info.purchase_cost %></td>
        <td><%= lottery_info.lottery_number %></td>
        <td><%= link_to 'Show', lottery_info %></td>
        <td><%= link_to 'Edit', edit_lottery_info_path(lottery_info) %></td>
        <td><%= link_to 'Destroy', lottery_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lottery Info', new_lottery_info_path %>
